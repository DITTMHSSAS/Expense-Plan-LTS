{
  "type": "calculated",
  "name": "ОшибкаКратко",
  "dataType": "string",
  "isDataTypeInferred": true,
  "expression": [
    "VAR tVal=",
    "IF('_АналогиРЖД'[ОшибкаДубль]<> \"Ок\", \"Д, \", \"\" )&",
    "IF('_АналогиРЖД'[ОшибкаЭталон=Аналог]<> \"Ок\", \"Э=А, \", \"\" )&",
    "IF('_АналогиРЖД'[ОшибкаКоэффициент] <>\"Ок\", \"К, \", \"\" )&",
    "IF('_АналогиРЖД'[ОшибкаЭталонЕд] <>\"Ок\", \"ЭЕд, \", \"\" )&",
    "IF('_АналогиРЖД'[ОшибкаЭталонИмя]<>\"Ок\", \"ЭИ, \", \"\" )",
    "RETURN",
    "IF(LEN(tVal)>0,LEFT(tVal,LEN( tVal )-2),\"Ок\")"
  ],
  "displayFolder": "ПроверкаАналогов"
}