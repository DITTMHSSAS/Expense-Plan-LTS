      "        \"_НоменклатураНСИ\", '_НормативыЦУНР_РЖД'[НоменклатураНСИ],",
      "        \"Норматив неснижаемого запаса, дней\",  BLANK(),",
      "        \"Основание\",  BLANK(),",
      "        \"Подгруппа ТМЦ\",  BLANK(),",
      "        \"Подразделение\",  BLANK(),",
      "        \"РесурснаяСпецификация\",  '_НормативыЦУНР_РЖД'[_КлючИмяФайлаЛист],",
      "        \"Сверхцикл\",  BLANK(),",
      "        \"Сезонность\",  \"Всесезонная\",",
      "        \"СтатусНоменклатуры\",  BLANK(),",
      "        \"СтатусРесурснойСпецификации\",  BLANK(),",
      "        \"ТарифныйРазряд\",  BLANK(),",
      "        \"ТипПозицииНоменклатуры\",  BLANK(),",
      "        \"ТипСпецификации\",  \"ЦУНР_РЖД\",",
      "        \"_КоличествоОбязательное\",  BLANK(),",
      "        \"Серия\",  '_НормативыЦУНР_РЖД'[Серия],",
      "        \"СЛД\",  BLANK(),",
      "        \"МестоРемонта\",  BLANK(),",
      "        \"ЕдИзм\",'_НормативыЦУНР_РЖД'[ЕдИзмНорм],",
      "        \"НачалоДействияРС\",DATE(YEAR(BLANK()),MONTH(BLANK()),DAY(BLANK())),",
      "        \"КонецДействияРС\",DATE(YEAR(BLANK()),MONTH(BLANK()),DAY(BLANK())),",
      "        \"КлассОбъектаРемонта\",\"Секция\",",
      "        \"ОбъектРемонтаШаблон\",'_НормативыЦУНР_РЖД'[ОбъектРемонтаШаблон],",
      "        \"ОбъектРемонтаШаблонСерии\",BLANK(),",
      "        \"ПланируетсяОтдельно\",BLANK(),",
      "        \"ПодзапросИсточник\",BLANK()",
      ")",
      "RETURN",
      "--tBlank",
      "UNION(tTab2,tTab3,tTab4)"
    ]
  }
