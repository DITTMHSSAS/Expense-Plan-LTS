{
  "name": "_ФактыРемонтовАСУСГ",
  "mode": "import",
  "source": {
    "type": "query",
    "query": [
      "EVALUATE",
      "VAR tTab2=SELECTCOLUMNS(",
      "    SUMMARIZE( FILTER( 'ФактыРемонтовАСУСГ', 'ФактыРемонтовАСУСГ'[ГодРемонта]=2022),",
      "        'ФактыРемонтовАСУСГ'[КлассОбъектаРемонта],",
      "        'ФактыРемонтовАСУСГ'[ГодРемонта],",
      "        'ФактыРемонтовАСУСГ'[ВидРемонта],",
      "        'ФактыРемонтовАСУСГ'[СерияЛокомотива],",
      "        'ФактыРемонтовАСУСГ'[СЛД],",
      "        'ФактыРемонтовАСУСГ'[МестоРемонта],",
      "        \"_Количество\", [фDebug_КоличествоРемонтов],",
      "        \"_ДатаАктуальности\", [ДатаАктуальности]",
      "    ),",
      "    \"Источник\",\"АСУСГ\",",
      "    \"КлассОбъектаРемонта\",'ФактыРемонтовАСУСГ'[КлассОбъектаРемонта],",
      "    \"Год\", 'ФактыРемонтовАСУСГ'[ГодРемонта],",
      "    \"ВидРемонта\", 'ФактыРемонтовАСУСГ'[ВидРемонта],",
      "    \"СерияЛокомотива\", 'ФактыРемонтовАСУСГ'[СерияЛокомотива],",
      "    \"СЛД\",'ФактыРемонтовАСУСГ'[СЛД],",
      "    \"МестоРемонта\",'ФактыРемонтовАСУСГ'[МестоРемонта],",
      "    \"КоличествоРемонтов\", [_Количество],",
      "    \"_ДатаАктуальности\", [_ДатаАктуальности]",
      ")",
      "RETURN tTab2"
    ],
    "dataSource": "AnalysisServices TMH-SRV060_ЗаказыАСУСГ"
  }
}