{
  "type": "calculated",
  "name": "_ЦенаЗакупкиАСУСГ",
  "dataType": "decimal",
  "expression": [
    "VAR tKRGD = 'Затраты'[Код СКМТР]",
    "VAR tKNsi = 'Затраты'[Код НСИ]",
    "VAR tSld = 'Затраты'[СЛД]",
    "VAR tEd  = 'Затраты'[ЕдИзм]",
    "",
    "VAR tPriceType=\"Цена закупки для контроля заказа ТМЦ\"",
    "",
    "VAR tTable1 =",
    "    FILTER(",
    "        '_ЦеныАСУСГ',",
    "            '_ЦеныАСУСГ'[КлассификаторТиповЦен] = \"Цена закупки для контроля заказа ТМЦ\"",
    "            && tKNsi = '_ЦеныАСУСГ'[КодНСИ]",
    "            && tSld = '_ЦеныАСУСГ'[ТипЦенСЛД]",
    "    )",
    "",
    "VAR tTable2 =",
    "    FILTER(",
    "        '_ЦеныРЖД',",
    "            tKRGD ='_ЦеныРЖД'[СКМТР]",
    "            &&UPPER( tEd )=UPPER( '_ЦеныРЖД'[Ед_изм] )",
    "    )",
    "",
    "VAR  tRet1 = MAXX( tTable1, '_ЦеныАСУСГ'[Цена] )",
    "",
    "RETURN",
    "tRet1"
  ],
  "displayFolder": "Техническая"
}