{
  "name": "Количество номенклатур без цены",
  "expression": "CALCULATE([Количество различных номенклатур],'Затраты'[Подбор_ЦеныСценарий1]=\"!Цена не подобрана\",'Затраты'[Подбор_ЦеныСценарий2]=\"!Цена не подобрана\")",
  "formatString": "#,##0",
  "detailRowsDefinition": {
    "expression": [
      "VAR  tTable = SUMMARIZE(",
      "    FILTER( 'Затраты', 'Затраты'[Подбор_ЦеныСценарий1] = \"!Цена не подобрана\" && 'Затраты'[Подбор_ЦеныСценарий2] = \"!Цена не подобрана\" ),",
      "    'Затраты'[ВидИсточника],",
      "    --'Затраты'[ТипСпецификации],",
      "    --'Затраты'[_Источник],",
      "    'Затраты'[Вид ресурса],",
      "    'Затраты'[Код СКМТР],",
      "    'Затраты'[Код НСИ],",
      "    'Затраты'[_Номенклатура],",
      "    'Затраты'[ЕдИзм],",
      "    'Затраты'[Подбор_ЦеныСценарий1],",
      "    'Затраты'[Подбор_ЦеныСценарий2],",
      "    \"_КоличествоНаПрограмму\", [Количество на программу],",
      "    \"_КоличествоНаОдинРемонт\", [Количество на один ремонт],",
      "    \"_КоличествоРемонтов\", [Количество ремонтов],",
      "    \"_ЦенаСценарий1\", [Цена сценарий 1],",
      "    \"_Стоимость сценарий 1\", [Стоимость сценарий 1],",
      "    \"_ЦенаСценарий2\", [Цена сценарий 2],",
      "    \"_Стоимость сценарий 2\", [Стоимость сценарий 2]",
      ")",
      "RETURN",
      "SELECTCOLUMNS(tTable,",
      "    \"ВидИсточника\",'Затраты'[ВидИсточника],",
      "    --\"ТипСпецификации\",'Затраты'[ТипСпецификации],    ",
      "    --\"Источник\",'Затраты'[_Источник],",
      "    \"ВидРесурса\",'Затраты'[Вид ресурса],",
      "    \"Код СКМТР\",'Затраты'[Код СКМТР],",
      "    \"Код НСИ\",'Затраты'[Код НСИ],",
      "    \"Номенклатура\",'Затраты'[_Номенклатура],",
      "    \"ЕдИзм\", 'Затраты'[ЕдИзм],",
      "    \"Количество ремонтов\", [_КоличествоРемонтов],",
      "    \"Количество на программу\", [_КоличествоНаПрограмму],",
      "    \"Количество на один ремонт\", [_КоличествоНаОдинРемонт],",
      "    \"Цена сценарий 1\", [_ЦенаСценарий1],",
      "    \"Стоимость сценарий 1\", [_Стоимость сценарий 1],",
      "    \"Цена сценарий 2\", [_ЦенаСценарий2],",
      "    \"Стоимость сценарий 2\", [_Стоимость сценарий 2]       ",
      ")"
    ]
  }
}