{
  "name": "СверкаЕдиницИзмерения",
  "mode": "import",
  "source": {
    "type": "calculated",
    "expression": [
      "VAR tTable1=SUMMARIZE(",
      "FILTER('_НормативыЦУНР_РЖД','_НормативыЦУНР_РЖД'[_СтрокаСодержитНорму]),",
      "'_НормативыЦУНР_РЖД'[ЕдИзм],",
      "'_НормативыЦУНР_РЖД'[ЕдИзмНорм],",
      "\"Источник\",\"_НормативыЦУНР_РЖД\",",
      "\"Количество\",COUNTROWS('_НормативыЦУНР_РЖД')",
      ")",
      "",
      "VAR tTable2=SUMMARIZE(",
      "'_НормативыАСУСГпоМК',",
      "'_НормативыАСУСГпоМК'[ЕдИзм],",
      "'_НормативыАСУСГпоМК'[ЕдИзмНорм],",
      "\"Источник\",\"_НормативыАСУСГпоМК\",",
      "\"Количество\",COUNTROWS('_НормативыАСУСГпоМК')",
      ")",
      "",
      "VAR tTable3=SUMMARIZE(",
      "'_ЦеныАСУСГ',",
      "'_ЦеныАСУСГ'[ЕдиницаИзмерения],",
      "'_ЦеныАСУСГ'[ЕдИзмНорм],",
      "\"Источник\",\"_ЦеныАСУСГ\",",
      "\"Количество\",COUNTROWS('_ЦеныАСУСГ')",
      ")",
      "",
      "VAR tTable4=SUMMARIZE(",
      "'_ЦеныЕРП',",
      "'_ЦеныЕРП'[ЕдиницаИзмерения],",
      "'_ЦеныЕРП'[ЕдИзмНорм],",
      "\"Источник\",\"_ЦеныЕРП\",",
      "\"Количество\",COUNTROWS('_ЦеныЕРП')",
      ")",
      "",
      "VAR tTable5=SUMMARIZE(",
      "'_ЦеныЛТС',",
      "'_ЦеныЛТС'[Ед_изм],",
      "'_ЦеныЛТС'[ЕдИзмНорм],",
      "\"Источник\",\"_ЦеныЛТС\",",
      "\"Количество\",COUNTROWS('_ЦеныЛТС')",
      ")",
      "",
      "VAR tTable6=SUMMARIZE(",
      "'_ЦеныРЖД_Сводный',",
      "'_ЦеныРЖД_Сводный'[ЕдИзм],",
      "'_ЦеныРЖД_Сводный'[ЕдИзмНорм],",
      "\"Источник\",\"_ЦеныРЖД_Сводный\",",
      "\"Количество\",COUNTROWS('_ЦеныРЖД_Сводный')",
      ")",
      "",
      "RETURN",
      "UNION(",
      "tTable1,tTable2,tTable3,tTable4,tTable5,tTable6",
      ")"
    ]
  }
}