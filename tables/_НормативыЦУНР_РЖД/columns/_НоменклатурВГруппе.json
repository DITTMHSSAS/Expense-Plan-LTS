{
  "type": "calculated",
  "name": "_НоменклатурВГруппе",
  "dataType": "int64",
  "isDataTypeInferred": true,
  "expression": [
    "VAR tVAl='_НормативыЦУНР_РЖД'[КодСКМТР]",
    "VAR tTab=FILTER(ALL('_НормативыЦУНР_РЖД'),tVAl='_НормативыЦУНР_РЖД'[КодСКМТР]",
    "                                          &&'_НормативыЦУНР_РЖД'[_СтрокаСодержитНорму]",
    "                                          &&ISBLANK('_НормативыЦУНР_РЖД'[КодНСИ])",
    ")",
    "RETURN ",
    "COUNTROWS(SUMMARIZE(tTab,",
    "    '_НормативыЦУНР_РЖД'[_Наименование],",
    "    '_НормативыЦУНР_РЖД'[МаркаМатериала],",
    "    '_НормативыЦУНР_РЖД'[ГОСТ_ТУ],",
    "    '_НормативыЦУНР_РЖД'[СортРазмер],",
    "    '_НормативыЦУНР_РЖД'[ЕдИзмНорм])",
    ")"
  ]
}