{
  "name": "СравнениеСправочниковЦен",
  "mode": "import",
  "source": {
    "type": "calculated",
    "expression": [
      "SUMMARIZE(FILTER('Затраты','Затраты'[!СЦЕНАРИЙ]=\"ЦУНР_РЖД, Годовой план 2023\"),",
      "'Затраты'[!СЦЕНАРИЙ],",
      "'Затраты'[Код СКМТР],",
      "'Затраты'[_Номенклатура],",
      "'Затраты'[Номенклатура],",
      "'Затраты'[ЕдИзм],",
      "'Затраты'[Подбор_ЦеныСценарий1],",
      "'Затраты'[Подбор_ЦеныСценарий2],",
      "'Затраты'[Подбор_ЦеныСценарий3],",
      "\"Стоимость сценарий 1\",[Стоимость сценарий 1],",
      "\"Стоимость сценарий 2\",[Стоимость сценарий 2],",
      "\"Стоимость сценарий 3\",[Стоимость сценарий 3],",
      "\"Цена сценарий 1\",[Цена сценарий 1],",
      "\"Цена сценарий 2\",[Цена сценарий 2],",
      "\"Цена сценарий 3\",[Цена сценарий 3],",
      "\"Количество на программу\",[Количество на программу],",
      "\"Количество на один ремонт\",[Количество на один ремонт],",
      "\"Отклонение\",VAR tAv=DIVIDE([Стоимость сценарий 1]+[Стоимость сценарий 2]+[Стоимость сценарий 3],3)",
      "      RETURN ABS([Стоимость сценарий 1]-tAv)+ABS([Стоимость сценарий 2]-tAv)+ABS([Стоимость сценарий 3]-tAv)",
      ")"
    ]
  }
}